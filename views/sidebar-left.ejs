<div class="sidebar-inner c-overflow">
    <div class="profile-menu" ng-controller="profileController">
        <a href="" toggle-submenu>
            <div class="profile-pic" >

                <img ng-if="profile.picture" src={{profile.picture}} class="img-circle dropdown-toggle margin10" data-toggle="dropdown" alt="USERNAME"
                     width="40" height="40" />

               <!-- <img ng-if="photo==false" src="" alt="">-->

                <ng-avatar ng-if="!profile.picture && prof" initials={{letters}} picture-format="png" colors-palette="color_palette" auto-color="true" round-shape="true"  width="40" height="40"></ng-avatar>


            </div>


            <div class="profile-info">
                {{profile.first_name}}

                <i class="zmdi zmdi-caret-down"></i>
            </div>
        </a>

        <ul class="main-menu" ng-controller="sidebarController">
            <li data-ui-sref-active="active">
                <a data-ui-sref="user_profile.user_profile_about" data-ng-click="mactrl.sidebarStat($event)"><i class="zmdi zmdi-account"></i>Mi Perfil</a>
            </li>


            <li>

              <a href="" ng-click=""><i class="zmdi zmdi-lock zmdi-hc-fw"></i>Seguridad y Privacidad</a>

            </li>

            <li>

                <a data-ui-sref="change_password" ng-click=""><i class="zmdi zmdi-key zmdi-hc-fw"></i>Contrase&ntilde;a</a>


            </li>

            <li>
               <a href="" ng-click="logout()"><i class="zmdi zmdi-time-restore"></i> Salir</a>
            </li>

        </ul>
    </div>

    <ul class="main-menu">
        
        <li data-ui-sref-active="active">
            <a data-ui-sref="home" data-ng-click="mactrl.sidebarStat($event)"><i class="zmdi zmdi-home"></i>Home</a>
        </li>
        <!--<li data-ui-sref-active="active" ng-if="'USER_ROLE' == 'root' || 'USER_ROLE' == 'sadmin'" >
            <a data-ui-sref="admins" data-ng-click="mactrl.sidebarStat($event)"><i class="zmdi zmdi-shield-security"></i> Administradores</a>
        </li>-->

        <!--<li data-ui-sref-active="active">
            <a data-ui-sref="user_profile" data-ng-click="mactrl.sidebarStat($event)"><i class="zmdi zmdi-calendar"></i>Mi Perfil</a>
        </li>-->

    </ul>
</div>
